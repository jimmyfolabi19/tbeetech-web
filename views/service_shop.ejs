<script>

    // Sample products data (you can replace this with actual product data from a database)
const products = [
    {
        id: 1,
        title: 'AI Product 1',
        description: 'Description of AI Product 1.',
        price: 199,
        image: 'ai_product.jpg',
    },
    {
        id: 2,
        title: 'Android Product 1',
        description: 'Description of Android Product 1.',
        price: 149,
        image: 'android_product.jpg',
    },
    // Add more product objects here for each service
];

// Sample user data (you can replace this with actual user authentication)
let loggedIn = false;

// Sample cart data (you can replace this with a more structured cart)
const cart = [];

document.addEventListener('DOMContentLoaded', () => {
    const loginLink = document.querySelector('.user-login');
    const logoutLink = document.querySelector('.user-logout');
    const profileLink = document.querySelector('.user-profile');
    const cartLink = document.querySelector('.user-cart');
    const cartItems = document.getElementById('cart-items');
    const cartTotal = document.getElementById('cart-total');
    const addToCartButtons = document.querySelectorAll('.add-to-cart');

    // Handle user login/logout logic (sample)
    loginLink.addEventListener('click', () => {
        loggedIn = true;
        toggleUserLinks();
    });

    logoutLink.addEventListener('click', () => {
        loggedIn = false;
        toggleUserLinks();
    });

    function toggleUserLinks() {
        if (loggedIn) {
            loginLink.style.display = 'none';
            logoutLink.style.display = 'inline';
            profileLink.style.display = 'inline';
        } else {
            loginLink.style.display = 'inline';
            logoutLink.style.display = 'none';
            profileLink.style.display = 'none';
        }
    }

    // Add products to the cart
    addToCartButtons.forEach(button => {
        button.addEventListener('click', () => {
            const productId = parseInt(button.getAttribute('data-product-id'));
            const product = products.find(p => p.id === productId);

            if (product) {
                cart.push(product);
                updateCartDisplay();
            }
        });
    });

    // Update cart display
    function updateCartDisplay() {
        cartItems.innerHTML = '';
        let total = 0;

        cart.forEach(item => {
            const listItem = document.createElement('li');
            listItem.textContent = `${item.title} - $${item.price}`;
            cartItems.appendChild(listItem);
            total += item.price;
        });

        cartTotal.textContent = `$${total.toFixed(2)}`;
    }
});

</script>
